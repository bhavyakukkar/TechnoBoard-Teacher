{
  "manifest_version": 2,

  "name": "TechnoBoard",
  "description": "This extension helps in conducting online meetings and classes",
  "version": "0.1.0",

  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "technoboard(16x16px).png"
  },

  "icons": {
  "16": "technoboard(16x16px).png",    
  "48": "technoboard(48x48px).png"
  },

  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["https://christuniversity.webex.com/wbxmjs/joinservice/sites/christuniversity/meeting/download/*"],
      "js": ["student/student-js/s-login.js"]
    }
  ]
}